	component HK_EPS_ModeManager HKEPS_MODMNG 
	component s3_5 S3_5 
	component s3_6 S3_6 
	component s15_1 S15_1 
	component s15_2 S15_2 

 
	/******** HK_EPS ***********/
	connector RDV3 HKEPS_read_HK ( HKEPS_MUX.take ,HKEPS_ACTFLAB.read_HK ,HKEPS_MODMNG.read_HK   )  
	connector RDV3 HKEPS_mem_res_fail ( HKEPS_MUX.release ,HKEPS_ACTFLAB.mem_res ,HKEPS_FAILMON.success   )  
	connector RDV3 HKEPS_mem_res_return ( HKEPS_MUX.release ,HKEPS_ACTFLAB.mem_res ,HKEPS_FAILMON.success   )  
	/* start: HK_to_I2C, MSG_LIB, I2C_SAT */
	connector RDV2 HKEPS_I2C_ask ( HKEPS_ACTFLAB.I2C_ask_EPS ,MSGLIB.composeMessage   )  
	connector RDV2 HKEPS_I2C_res ( HKEPS_ACTFLAB.I2C_res_EPS ,MSGLIB.decodeMessage   )  
	connector RDV4 HKEPS_I2C_fail_error ( HKEPS_ACTFLAB.I2C_fail_EPS ,HKEPS_MUX.release ,HKEPS_FAILMON.failure ,MSGLIB.decodeMessage   )  
	connector SINGLE HKEPS_I2C_nofail_ask ( HKEPS_FAILMON.I2C_ask_EPS   )  
	connector SINGLE HKEPS_I2C_nofail_res ( HKEPS_FAILMON.I2C_res_EPS   )  
	connector SINGLE HKEPS_I2C_nofail_error ( HKEPS_FAILMON.I2C_res_EPS   )  
	connector RDV2 HKEPS_ask_I2C_TTC ( HKEPS_ACTFLAB.ask ,HKEPS_PSMODMNG.ask_I2C_TTC   )  
	connector RDV3 HKEPS_mem_write_req ( HKEPS_ACTFLAB.ask ,HKEPS_PSMODMNG.mem_write_req ,MEMLIB.setWrite   )  
	/* TODO for all: add in .smv file : ...pl2 > 1 */
