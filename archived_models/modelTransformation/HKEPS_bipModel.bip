	connector SINGLE MEMLIB_checkCRC ( MEMLIB.checkCRC   )  
	connector SINGLE MEMLIB_setRead ( MEMLIB.setRead   )  
	connector SINGLE HKEPS_ACTFLAB_I2C_res_TTC ( HKEPS_ACTFLAB.I2C_res_TTC   )  
	connector SINGLE S3_5_com ( S3_5.com   )  
