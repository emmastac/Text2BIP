MODULE main

	VAR
		NuInteraction		:		{NuI1, NuI2, NuI3, NuI4, NuI5, NuI6, NuI7, NuI8, NuI9, NuI10, NuI11, NuI12, NuI13, NuI14, NuI15, NuI16};
		tcmod	:	TCMOD(NuInteraction);
		tcmng	:	TCMNG(NuInteraction);
		tcfifo	:	TCFIFO(NuInteraction);
		smutc	:	SMUTC(NuInteraction);

	DEFINE
		TCFIFO_internal2		:=		tcfifo.NuPinternal2;
		ask		:=		smutc.NuPask;
		acqSg_e		:=		( (smutc.NuPget_e) & (tcmng.NuPacqSg_e) );
		acqFail_e		:=		tcmng.NuPacqFail_e;
		fullFifo1		:=		( (tcmng.NuPput) & (tcfifo.NuPfull) );
		emptySg		:=		( (smutc.NuPempty) & (tcmng.NuPnoSg) );
		TCFIFO_internal1		:=		tcfifo.NuPinternal1;
		success		:=		tcmng.NuPsuccess;
		TCFIFO_internal3		:=		tcfifo.NuPinternal3;
		nfullFifo2		:=		( (( (tcfifo.NuPget) & (tcmng.NuPgetGround) )) & (tcmod.NuPtoNFull) );
		emptyGround		:=		( (tcfifo.NuPempty) & (tcmng.NuPgetGround) );
		acqSg_b		:=		( (( (smutc.NuPget_b) & (tcmng.NuPacqSg_b) )) & (tcmod.NuPacqSg) );
		storeTC		:=		( (tcmng.NuPput) & (tcfifo.NuPput) );
		fullFifo2		:=		( (( (tcmng.NuPput) & (tcfifo.NuPfull) )) & (tcmod.NuPtoFull) );
		nfullFifo1		:=		( (tcfifo.NuPget) & (tcmng.NuPgetGround) );
		acqFail_b		:=		( (tcmng.NuPacqFail_b) & (tcmod.NuPacqFail) );


	INVAR
		( (( (NuInteraction) = (NuI14) )) -> (TCFIFO_internal2) ) &
		( (( (NuInteraction) = (NuI1) )) -> (ask) ) &
		( (( (NuInteraction) = (NuI6) )) -> (acqSg_e) ) &
		( (( (NuInteraction) = (NuI7) )) -> (acqFail_e) ) &
		( (( (NuInteraction) = (NuI4) )) -> (fullFifo1)  & !(fullFifo2)) &
		( (( (NuInteraction) = (NuI8) )) -> (emptySg) ) &
		( (( (NuInteraction) = (NuI15) )) -> (TCFIFO_internal1) ) &
		( (( (NuInteraction) = (NuI10) )) -> (success)  & !(acqFail_b)) &
		( (( (NuInteraction) = (NuI16) )) -> (TCFIFO_internal3) ) &
		( (( (NuInteraction) = (NuI11) )) -> (nfullFifo2) ) &
		( (( (NuInteraction) = (NuI13) )) -> (emptyGround) ) &
		( (( (NuInteraction) = (NuI5) )) -> (acqSg_b) ) &
		( (( (NuInteraction) = (NuI2) )) -> (storeTC) ) &
		( (( (NuInteraction) = (NuI3) )) -> (fullFifo2) ) &
		( (( (NuInteraction) = (NuI12) )) -> (nfullFifo1)  & !(nfullFifo2)) &
		( (( (NuInteraction) = (NuI9) )) -> (acqFail_b) );

		
/-- AG (  TCmanagement.ask -> AX A[ !( TCmanagement.ask  ) U ( TCmanagement.acqSg_e \/ TCmanagement.noSg] )  --/
FAIRNESS 	(NuInteraction) = (NuI6)  | (NuInteraction) = (NuI8);			
CTLSPEC AG ( (NuInteraction) = (NuI1) -> AX A[ !( (NuInteraction) = (NuI1) ) U ( (NuInteraction) = (NuI6)  | (NuInteraction) = (NuI8) ) ] )

/-- AG (  TCmanagement.put -> AX A[ !( TCmanagement.put  ) U ( TCmanagement.acqFail_e \/ TCmanagement.success] )  --/
FAIRNESS 	(NuInteraction) = (NuI7)  | (NuInteraction) = (NuI10);			
CTLSPEC AG ( (NuInteraction) = (NuI2) -> AX A[ !( (NuInteraction) = (NuI2) ) U ( (NuInteraction) = (NuI7)  | (NuInteraction) = (NuI10) ) ] )






